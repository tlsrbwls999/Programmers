def solution(land):
    n=len(land)
    m=4
    for i in range(1,n):
        for j in range(m):
            a=land[i][j]
            for k in range(m):
                if j!=k:
                    land[i][j]=max(land[i][j],a+land[i-1][k])
    max_num=0
    for a in range(m):
        if land[n-1][a]>max_num:
            max_num=land[n-1][a]
    return max_num
